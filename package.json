{
  "name": "language-nlf",
  "version": "3.1.1",
  "description": "Atom language support for NSIS Language Files",
  "license": "MIT OR GPL-2.0",
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "nsis",
    "nullsoft",
    "installer",
    "setup"
  ],
  "scripts": {
    "build:grammar": "node tools/build-grammar.cjs",
    "build:snippets": "node tools/build-snippets.cjs",
    "build": "npm-run-all --parallel build:*",
    "postinstall": "npm run build",
    "precommit": "npm run test",
    "lint": "eslint ./tools/*.cjs",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-language-nlf"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "language-nsis"
  ],
  "dependencies": {
    "@nsis/language-data": "^0.8.0",
    "atom-package-deps": "^5.0.0",
    "cson": "^7.20.0",
    "log-symbols": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "to-capital-case": "^1.0.0",
    "to-slug-case": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-plugin-json": "^3.1.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2"
  },
  "lint-staged": {
    "*.cjs": "eslint --cache --fix"
  }
}
